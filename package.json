{
  "name": "@openresume/theme",
  "version": "0.1.11",
  "description": "A theme package for OpenResume",
  "main": "dist/exports.js",
  "module": "dist/exports.mjs",
  "types": "dist/exports.d.ts",
  "exports": {
    ".": {
      "import": "./dist/exports.mjs",
      "require": "./dist/exports.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "next dev",
    "prebuild": "tsx scripts/generateRobotsTxt.ts",
    "build": "next build",
    "build-lib": "tsup src/exports.ts --format esm,cjs --dts --tsconfig tsconfig.build.json --external react",
    "start": "next start",
    "check": "npm run types:check && npm run lint && npm run prettier && npm run test",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "types:check": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/missionmike/openresume-theme.git"
  },
  "keywords": [
    "resume",
    "theme",
    "openresume"
  ],
  "author": "Michael R. Dinerstein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/missionmike/openresume-theme/issues"
  },
  "homepage": "https://github.com/missionmike/openresume-theme#readme",
  "dependencies": {
    "@iconify/react": "^5.0.2",
    "@mui/base": "^5.0.0-beta.68",
    "@mui/icons-material": "^6.2.0",
    "@mui/material": "^6.1.9",
    "@mui/material-nextjs": "^6.1.8",
    "@mui/x-date-pickers": "^7.24.1",
    "@next/third-parties": "^15.0.3",
    "dayjs": "^1.11.13",
    "html-react-parser": "^5.2.1",
    "html2pdf.js": "^0.10.2",
    "next": "15.0.3",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-dom": "^18.3.1",
    "sass": "^1.81.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18.3.12",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^18.3.1",
    "@whatwg-node/fetch": "^0.10.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
